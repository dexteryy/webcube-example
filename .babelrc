{
  "presets": [
    ["env", {
      "targets": {
        "node": "8.1.2"
      },
      "include": [],
      "useBuiltIns": true,
      "loose": false,
      "debug": false
    }],
    "react"
  ],
  "plugins": [
    "add-module-exports",
    "transform-es2015-modules-commonjs",
    "transform-function-bind",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-decorators-legacy"
  ]
}
