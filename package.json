{
  "name": "webcube-example",
  "description": "",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/webcube-example/issues"
  },
  "homepage": "https://github.com/dexteryy/webcube-example",
  "repository": "https://github.com/dexteryy/webcube-example",
  "version": "0.1.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "empty": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js clean:empty",
    "lint": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js check:all",
    "test": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js test:all",
    "test:quick": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js test:functional",
    "test:unit": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js test:unit",
    "test:cloud": "NODE_ENV=cloud gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js test:functional",
    "build": "npm run start:staticserver && gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js build",
    "build:quick": "npm run start:staticserver && gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js build:html",
    "new": "plop --plopfile=node_modules/webcube/configs/plopfile.babel.js",
    "dev": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js watch:dev",
    "start:staticserver": "nohup gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js start:staticserver > ./webserver.log 2>&1 &",
    "stop:staticserver": "gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js stop:staticserver",
    "deploy:staticweb": "NODE_ENV=cloud gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js deploy:staticweb",
    "redeploy:staticweb": "NODE_ENV=cloud gulp --gulpfile node_modules/webcube/configs/gulpfile.babel.js redeploy:staticweb"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6.1.0"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "classnames": "^2.2.5",
    "normalize.css": "^4.2.0",
    "purecss": "^0.6.0",
    "semantic-ui-css": "^2.2.2",
    "webcube": "^0.4.7"
  },
  "devDependencies": {},
  "keywords": [
    "boilerplate",
    "starter",
    "offline",
    "offline-first",
    "staticweb",
    "react",
    "react",
    "gulp",
    "webpack",
    "babel",
    "linter"
  ]
}
